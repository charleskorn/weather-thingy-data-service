{
  "acKind": "ImageManifest",
  "acVersion": "0.5.1",
  "name": "charleskorn.com/weather-thingy/data-service",
  "labels": [
    {
      "name": "version",
      "value": "1.0.0"
    },
    {
      "name": "arch",
      "value": "amd64"
    },
    {
      "name": "os",
      "value": "linux"
    }
  ],
  "app": {
    "user": "root",
    "group": "root",
    "exec": [
      "/bin/weather-thingy-data-service",
      "-address",
      ":8000"
    ],
    "ports": [
      {
        "name": "www",
        "protocol": "tcp",
        "port": 8000
      }
    ]
  },
  "annotations": [
    {
      "name": "authors",
      "value": "Charles Korn <charleskorn@gmail.com>"
    }
  ]
}
